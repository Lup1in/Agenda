<% if (errors.length> 0 ) { %>
    <div class="errorDIV">
        <% errors.forEach(erro=> { %>
            <%= erro %> <br>
                <% }); %>
    </div>
    <% } %>

        <% if (success.length> 0 ) { %>
            <div class="sucessoDIV">
                <% success.forEach(success=> { %>
                    <%= success %> <br>
                        <% }); %>
            </div>
            <% } %>